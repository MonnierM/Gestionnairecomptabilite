                android:layout_width="match_parent"
                android:textColor="@color/blackcolor"
                android:textStyle="bold"
                android:textSize="20sp"
                android:layout_height="50dp"/>
            <EditText
                android:id="@+id/caisse_debit7"
                android:layout_width="match_parent"
                android:textColor="@color/blackcolor"
                android:textStyle="bold"

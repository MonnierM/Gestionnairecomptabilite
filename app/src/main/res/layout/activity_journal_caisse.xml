                android:layout_width="match_parent"
                android:textColor="@color/blackcolor"
                android:textSize="20sp"
                android:layout_height="50dp"/>
            <EditText
                android:id="@+id/caisse_date5"
                android:layout_width="match_parent"
                android:textColor="@color/blackcolor"
                android:textSize="20sp"
                android:layout_height="50dp"/>
            <EditText
                android:id="@+id/caisse_date6"
                android:layout_width="match_parent"
                android:textColor="@color/blackcolor"
                android:textSize="20sp"
                android:layout_height="50dp"/>
            <EditText

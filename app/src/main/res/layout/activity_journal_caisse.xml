                android:layout_height="50dp"/>
            <EditText
                android:id="@+id/caisse_credit10"
                android:layout_width="match_parent"
                android:textColor="@color/blackcolor"
                android:textSize="20sp"
                android:textStyle="bold"
                android:layout_height="50dp"/>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>